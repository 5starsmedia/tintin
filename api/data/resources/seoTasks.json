{
  "resource": "seoTasks",
  "model": "SeoTask",
  "options": {
    "searchFields": [
      {
        "role": "admin",
        "value": ["msg", "hostname", "req.remoteAddress", "req.userAgent.browser", "req.userAgent.os"]
      }
    ],
    "maxPerPage": [
      {
        "role": "admin",
        "value": 200
      }
    ]
  },
  "acl": {
    "*": {
      "get": "*",
      "get?collectionName&resourceId": "*",
      "get?_id": "*",
      "put?_id": ["admin", "*:owner"],
      "post": "admin",
      "delete?_id": "admin"
    }
  }
}